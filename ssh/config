    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_yubikey.pub

Host output
  HostName 127.0.0.1
  User vagrant
  Port 2200
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/hootieben/.vagrant.d/insecure_private_key
  IdentitiesOnly yes
  LogLevel FATAL

